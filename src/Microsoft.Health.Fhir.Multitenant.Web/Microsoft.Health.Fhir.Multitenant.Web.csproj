    <PackageReference Include="System.Text.Encodings.Web" />
    <PackageReference Include="Microsoft.AspNetCore.Http" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.Health.Fhir.Multitenant.Core\Microsoft.Health.Fhir.Multitenant.Core.csproj" />
    <ProjectReference Include="..\Microsoft.Health.Fhir.Api.OpenIddict\Microsoft.Health.Fhir.Api.OpenIddict.csproj" />
